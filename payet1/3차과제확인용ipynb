estAccount:\n","    balance = 100\n","    @validate_transaction\n","    def deposit(self, amount: int):\n","        print(f\"{amount}원 입금을 처리합니다.\")\n","    @validate_transaction\n","    def withdraw(self, amount: int):\n","        if self.balance < amount:\n","            raise InsufficientFundsError(self.balance, amount)\n","        print(f\"{amount}원 출금을 처리합니다.\")\n","print(\"--- 데코레이터 및 예외 처리 테스트 ---\")\n","test_acc = TestAccount()\n","try:\n","    test_acc.deposit(50)\n","    test_acc.withdraw(200)\n","except InsufficientFundsError as e:\n","    print(f\"오류 발생: {e}\")\n","try:\n","    test_acc.deposit(-10)\n","except NegativeAmountError as e:\n","    print(f\"오류 발생: {e}\")"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"LigIhAP9m6Js","executionInfo":{"status":"ok","timestamp":1756719223476,"user_tz":-540,"elapsed":15,"user":{"displayName":"이이현직","userId":"10231870271723839000"}},"outputId":"b623f785-7d2a-4578-c203-54fc2e6b5c5b"},"execution_count":null,"outputs":[{"output_type":"stream","name":"stdout","text":["--- 데코레이터 및 예외 처리 테스트 ---\n","50원 입금을 처리합니다.\n","오류 발생: 잔고 부족. 현재 잔액: 100, 출금 요청액: 200\n","오류 발생: 금액은 0보다 커야 합니다.\n"]}]},{"cell_type":"code","source":["class InsufficientFundsError(Exception):\n","    def __init__(self, balance: int, amount: int) -> None:\n","        super().__init__(f\"잔고 부족. 현재 잔액: {balance}, 출금 요청액: {amount}\")\n","class NegativeAmountError(Exception):\n","\n","    def __init__(self) -> None:\n","        super().__init__(\"금액은 0보다 커야 합니다.\")\n","\n","class UserNotFoundError(Exception):\n","    def __init__(self, username: str) -> None:\n","        super().__init__(f\"사용자를 찾을 수 없습니다: {username}\")"],"metadata":{"id":"5qNqRYXukWeY"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["from typing import Callable\n","class InsufficientFundsError(Exception):\n","    def __init__(self, balance: int, amount: int): super().__init__(f\"잔고 부족. 잔액:{balance}, 요청액:{amount}\")\n","class NegativeAmountError(Exception):\n","    def __init__(self): super().__init__(\"금액은 0보다 커야 합니다.\")\n","class UserNotFoundError(Exception):\n","    def __init__(self, username: str): super().__init__(f\"사용자 없음: {username}\")\n","def validate_transaction(func: Callable) -> Callable:\n","    def wrapper(self, amount: int):\n","        if amount <= 0: raise NegativeAmountError()\n","        return func(self, amount)\n","    return wrapper\n","class Transaction:\n","    def __init__(self, tt: str, a: int, b: int): self.tt, self.a, self.b = tt, a, b\n","    def __str__(self): return f\"유형: {self.tt}, 금액: {self.a}, 잔고: {self.b}\"\n","class Account:\n","    bank_name = \"멋쟁이 은행\"\n","    def __init__(self): self.__balance, self.transactions = 0, []\n","    @validate_transaction\n","    def deposit(self, a: int): self.__balance += a; self.transactions.append(Transaction(\"입금\", a, self.__balance)); print(f\"{a}원 입금 완료. 잔액:{self.__balance}\")\n","    @validate_transaction\n","    def withdraw(self, a: int):\n","        if self.__balance < a: raise InsufficientFundsError(self.__balance, a)\n","        self.__balance -= a; self.transactions.append(Transaction(\"출금\", a, self.__balance)); print(f\"{a}원 출금 완료. 잔액:{self.__balance}\")\n","    def get_balance(self): return self.__balance\n","    def get_transactions(self): return self.transactions\n","    @classmethod\n","    def get_bank_name(cls): return cls.bank_name\n","class User:\n","    def __init__(self, u: str): self.username, self.account = u, Account()\n","class BankingService:\n","    def __init__(self): self.users = []\n","    def add_user(self, u: str): self.users.append(User(u)); print(f\"'{u}'님, 계좌 생성 완료.\")\n","    def find_user(self, u: str):\n","        for user in self.users:\n","            if user.username == u: return user\n","        raise UserNotFoundError(u)\n","    def user_menu(self, u: str):\n","        try:\n","            user = self.find_user(u)\n","            while True:\n","                print(f\"\\n--- [{user.username}]님 메뉴 ---\")\n","                print(\"1.입금 | 2.출금 | 3.잔고 | 4.거래내역 | 5.로그아웃\")\n","                c = input(\"선택: \")\n","                try:\n","                    if c == '1': user.account.deposit(int(input(\"입금액: \")))\n","                    elif c == '2': user.account.withdraw(int(input(\"출금액: \")))\n","                    elif c == '3': print(f\"잔고: {user.account.get_balance()}원\")\n","                    elif c == '4':\n","                        txs = user.account.get_transactions()\n","                        if not txs: print(\"거래 내역 없음.\")\n","                        else: [print(t) for t in txs]\n","                    elif c == '5': print(\"로그아웃.\"); break\n","                    else: print(\"잘못된 선택.\")\n","                except (ValueError, NegativeAmountError, InsufficientFundsError) as e: print(f\"오류: {e}\")\n","        except UserNotFoundError as e: print(f\"오류: {e}\")\n","def main() -> None:\n","    \"\"\"온라인 뱅킹 시스템의 메인 로직을 실행합니다.\"\"\"\n","    service = BankingService()\n","    print(f\"** {Account.get_bank_name()} 온라인 뱅킹 시스템 **\")\n","    while True:\n","        print(\"\\n--- 메인 메뉴 ---\")\n","        print(\"1. 사용자 추가 | 2. 사용자 로그인 | 3. 종료\")\n","        choice = input(\"선택: \")\n","        if choice == '1': service.add_user(input(\"사용자 이름: \"))\n","        elif choice == '2': service.user_menu(input(\"로그인 이름: \"))\n","        elif choice == '3': print(\"시스템 종료.\"); break\n","        else: print(\"잘못된 선택.\")\n","\n","if __name__ == \"__main__\":\n","    main()"],"metadata":{"id":"EcEad0oVkWbl"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["import time\n","import sys\n","from typing import Callable\n","def processing_animation(duration=1):\n","    animation = \"|/-\\\\\"\n","    start_time = time.time()\n","    while time.time() < start_time + duration:\n","        for char in animation:\n","            sys.stdout.write(f\"\\r처리 중... {char}\")\n","            sys.stdout.flush()\n","            time.sleep(0.1)\n","    sys.stdout.write(\"\\r완료!          \\n\")\n","class InsufficientFundsError(Exception):\n","    def __init__(self, balance: int, amount: int): super().__init__(f\"잔고 부족. 잔액:{balance}, 요청액:{amount}\")\n","class NegativeAmountError(Exception):\n","    def __init__(self): super().__init__(\"금액은 0보다 커야 합니다.\")\n","class UserNotFoundError(Exception):\n","    def __init__(self, username: str): super().__init__(f\"사용자 없음: {username}\")\n","def validate_transaction(func: Callable) -> Callable:\n","    def wrapper(self, amount: int):\n","        if amount <= 0: raise NegativeAmountError()\n","        return func(self, amount)\n","    return wrapper\n","class Transaction:\n","    def __init__(self, tt: str, a: int, b: int): self.tt, self.a, self.b = tt, a, b\n","    def __str__(self): return f\"유형: {self.tt}, 금액: {self.a}, 잔고: {self.b}\"\n","class Account:\n","    bank_name = \"멋쟁이 은행\"\n","    def __init__(self): self.__balance, self.transactions = 0, []\n","    @validate_transaction\n","    def deposit(self, a: int):\n","        print(\"입금을 진행합니다...\")\n","        processing_animation()\n","        self.__balance += a\n","        self.transactions.append(Transaction(\"입금\", a, self.__balance))\n","        print(f\"✅ {a}원 입금 완료. 현재 잔액: {self.__balance}원\")\n","    @validate_transaction\n","    def withdraw(self, a: int):\n","        print(\"출금을 진행합니다...\")\n","        if self.__balance < a:\n","            raise InsufficientFundsError(self.__balance, a)\n","        processing_animation()\n","        self.__balance -= a\n","        self.transactions.append(Transaction(\"출금\", a, self.__balance))\n","        print(f\"✅ {a}원 출금 완료. 현재 잔액: {self.__balance}원\")\n","    def get_balance(self): return self.__balance\n","    def get_transactions(self): return self.transactions\n","    @classmethod\n","    def get_bank_name(cls): return cls.bank_name\n","class User:\n","    def __init__(self, u: str): self.username, self.account = u, Account()\n","class BankingService:\n","    def __init__(self): self.users = []\n","    def add_user(self, u: str):\n","        print(f\"'{u}'님 계좌를 생성합니다...\")\n","        processing_animation()\n","        self.users.append(User(u))\n","        print(f\"🎉 '{u}'님, 계좌 생성이 완료되었습니다.\")\n","    def find_user(self, u: str):\n","        for user in self.users:\n","            if user.username == u: return user\n","        raise UserNotFoundError(u)\n","    def user_menu(self, u: str):\n","        try:\n","            print(f\"'{u}'님 정보를 찾는 중...\")\n","            user = self.find_user(u)\n","            processing_animation(0.5)\n","            while True:\n","                print(f\"\\n--- [{user.username}]님 메뉴 ---\")\n","                print(\"1.입금 | 2.출금 | 3.잔고 | 4.거래내역 | 5.로그아웃\")\n","                c = input(\"선택: \")\n","                try:\n","                    if c == '1': user.account.deposit(int(input(\"입금액: \")))\n","                    elif c == '2': user.account.withdraw(int(input(\"출금액: \")))\n","                    elif c == '3': print(f\"현재 잔고: {user.account.get_balance()}원\")\n","                    elif c == '4':\n","                        txs = user.account.get_transactions()\n","                        print(\"\\n--- 거래 내역 ---\")\n","                        if not txs: print(\"거래 내역이 없습니다.\")\n","                        else: [print(t) for t in txs]\n","                        print(\"-----------------\")\n","                    elif c == '5': print(\"로그아웃합니다.\"); break\n","                    else: print(\"잘못된 선택입니다.\")\n","                except (ValueError, NegativeAmountError, InsufficientFundsError) as e:\n","                    print(f\"오류: {e}\")\n","        except UserNotFoundError as e:\n","            print(f\"오류: {e}\")\n","def main() -> None:\n","    service = BankingService()\n","    print(f\"** {Account.get_bank_name()} 온라인 뱅킹 시스템 **\")\n","    while True:\n","        print(\"\\n--- 메인 메뉴 ---\")\n","        print(\"1. 사용자 추가 | 2. 사용자 로그인 | 3. 종료\")\n","        choice = input(\"선택: \")\n","        if choice == '1': service.add_user(input(\"사용자 이름: \"))\n","        elif choice == '2': service.user_menu(input(\"로그인 이름: \"))\n","        elif choice == '3': print(\"시스템 종료.\"); break\n","        else: print(\"잘못된 선택.\")\n","if __name__ == \"__main__\":\n","    main()\n"],"metadata":{"id":"Ta4z7PorkWYp","colab":{"base_uri":"https://localhost:8080/","height":530},"executionInfo":{"status":"error","timestamp":1756720557040,"user_tz":-540,"elapsed":18754,"user":{"displayName":"이이현직","userId":"10231870271723839000"}},"outputId":"cb1d8b38-46c5-4b9a-fe5e-e5f750e7dbf1"},"execution_count":36,"outputs":[{"output_type":"stream","name":"stdout","text":["** 멋쟁이 은행 온라인 뱅킹 시스템 **\n","\n","--- 메인 메뉴 ---\n","1. 사용자 추가 | 2. 사용자 로그인 | 3. 종료\n","선택: 2\n","로그인 이름: 이현직\n","'이현직'님 정보를 찾는 중...\n","오류: 사용자 없음: 이현직\n","\n","--- 메인 메뉴 ---\n","1. 사용자 추가 | 2. 사용자 로그인 | 3. 종료\n"]},{"output_type":"error","ename":"KeyboardInterrupt","evalue":"Interrupted by user","traceback":["\u001b[0;31m---------------------------------------------------------------------------\u001b[0m","\u001b[0;31mKeyboardInterrupt\u001b[0m                         Traceback (most recent call last)","\u001b[0;32m/tmp/ipython-input-1670799593.py\u001b[0m in \u001b[0;36m<cell line: 0>\u001b[0;34m()\u001b[0m\n\u001b[1;32m     98\u001b[0m         \u001b[0;32melse\u001b[0m\u001b[0;34m:\u001b[0m \u001b[0mprint\u001b[0m\u001b[0;34m(\u001b[0m\u001b[0;34m\"잘못된 선택.\"\u001b[0m\u001b[0;34m)\u001b[0m\u001b[0;34m\u001b[0m\u001b[0;34m\u001b[0m\u001b[0m\n\u001b[1;32m     99\u001b[0m \u001b[0;32mif\u001b[0m \u001b[0m__name__\u001b[0m \u001b[0;34m==\u001b[0m \u001b[0;34m\"__main__\"\u001b[0m\u001b[0;34m:\u001b[0m\u001b[0;34m\u001b[0m\u001b[0;34m\u001b[0m\u001b[0m\n\u001b[0;32m--> 100\u001b[0;31m     \u001b[0mmain\u001b[0m\u001b[0;34m(\u001b[0m\u001b[0;34m)\u001b[0m\u001b[0;34m\u001b[0m\u001b[0;34m\u001b[0m\u001b[0m\n\u001b[0m","\u001b[0;32m/tmp/ipython-input-1670799593.py\u001b[0m in \u001b[0;36mmain\u001b[0;34m()\u001b[0m\n\u001b[1;